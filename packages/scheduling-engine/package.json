{
  "name": "@cronicorn/scheduling-engine",
  "version": "0.1.0",
  "description": "Intelligent scheduling engine for Cronicorn",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "lint:fix": "eslint . --max-warnings 0 --fix",
    "clean": "rm -rf dist node_modules",
    "start": "node dist/index.js",
    "start:dev": "tsx src/index.ts"
  },
  "peerDependencies": {
    "@tasks-app/api": "workspace:*"
  },
  "dependencies": {
    "@ai-sdk/openai": "2.0.10",
    "ai": "5.0.10",
    "cron-parser": "4.9.0",
    "drizzle-orm": "0.38.3",
    "node-fetch": "3.3.2",
    "p-queue": "7.4.1",
    "pino": "9.6.0",
    "uuid": "9.0.1",
    "vite-tsconfig-paths": "5.1.4",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@tasks-app/eslint-config": "workspace:*",
    "@types/node": "~22.10.2",
    "@types/uuid": "~9.0.7",
    "@vitest/coverage-v8": "~2.1.9",
    "eslint": "~9.31.0",
    "tsx": "~4.19.2",
    "typescript": "~5.8.3",
    "vitest": "~2.1.8"
  }
}
