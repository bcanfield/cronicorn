{
  "name": "tasks-app",
  "version": "1.0.0",
  "license": "MIT",
  "packageManager": "pnpm@10.7.0",
  "scripts": {
    "dev": "turbo run dev",
    "clean": "pnpm run -r --parallel clean && rm -rf node_modules",
    "build": "pnpm run -r build",
    "test": "pnpm run -r --parallel test",
    "lint": "pnpm run -r --parallel lint",
    "db:push": "pnpm run -r --parallel db:push",
    "deploy": "pnpm build && pnpm run -r deploy",
    "typecheck": "pnpm run -r --parallel typecheck"
  },
  "dependencies": {
    "turbo": "^2.5.5"
  }
}